{
  "name": "ag-grid-documentation",
  "private": true,
  "description": "The website for AG Grid",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "generate-examples": "cd .. && node dev-server.js generate-examples && cd documentation",
    "customise": "node customise.js",
    "build": "npm run clean && npm run customise",
    "package": "npm run generate-examples && npm run publish",
    "publish": "npm run build && npx gatsby build --prefix-paths",
    "develop": "npx gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run customise && npm run develop",
    "serve": "npx gatsby serve --prefix-paths -H 0.0.0.0",
    "clean": "npx gatsby clean",
    "test": "jest"
  },
